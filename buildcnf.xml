      <command>#!/bin/bash
curl --upload-file $WORKSPACE/webapp/target/*.war -v -u admin:admin123 -v http://40.76.223.109:8081/repository/nexusraw/</command>
    </hudson.tasks.Shell>
  </postbuilders>
  <runPostStepsIfResult>
    <name>SUCCESS</name>
    <ordinal>0</ordinal>
